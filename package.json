{
  "name": "recipes-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:gh": "vite build --base=/recipes-app",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "stylelint": "stylelint ./src/**/*.css --fix",
    "format": "npm run eslint:fix && npm run stylelint",
    "eslint:fix": "eslint --fix src/",
    "check": "eslint src/ && stylelint ./src/**/*.css",
    "generate-api": "openapi-generator-cli generate -i https://spoonacular.com/application/frontend/downloads/spoonacular-openapi-3.json -g typescript-fetch -o ./src/api"
  },
  "dependencies": {
    "dompurify": "^3.0.6",
    "react": "^19.0.0-canary-33a32441e9-20240418",
    "react-dom": "^19.0.0-canary-33a32441e9-20240418",
    "react-error-boundary": "^4.0.13",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^15.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.4",
    "prettier-eslint": "^16.2.0",
    "stylelint": "^16.2.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  }
}

{
  "name": "recipes-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:gh": "npm run generate-api && vite build --base=/recipes-app",
    "preview": "vite preview",
    "generate-api": "openapi-generator-cli generate -i https://spoonacular.com/application/frontend/downloads/spoonacular-openapi-3.json -g typescript-fetch -o ./src/api"
  },
  "dependencies": {
    "dompurify": "^3.0.6",
    "preact": "^10.19.3",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@preact/preset-vite": "^2.7.0",
    "@testing-library/preact": "^3.2.3",
    "@types/dompurify": "^3.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.7"
  }
}
